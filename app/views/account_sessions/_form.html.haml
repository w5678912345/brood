= form_for @account_session do |f|
  - if @account_session.errors.any?
    #error_explanation
      %h2= "#{pluralize(@account_session.errors.count, "error")} prohibited this account_session from being saved:"
      %ul
        - @account_session.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :started_at
    = f.datetime_select :created_at
  .field
    = f.label :started_status
    = f.text_field :started_status
  .field
    = f.label :finished_at
    = f.datetime_select :finished_at
  .field
    = f.label :finished
    = f.check_box :finished
  .field
    = f.label :finished_status
    = f.text_field :finished_status
  .field
    = f.label :ip
    = f.text_field :ip

  .field
    = f.label :remark
    = f.text_field :remark
  .actions
    = f.submit 'Save'
