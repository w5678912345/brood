<%= render "navbar" %>

<form class="form-search" action="<%= analysis_notes_path %>">
<div class="input-prepend input-append">
<span class="add-on"><i class="icon-calendar"></i></span>
<input type="text" name="start_date" id="start_date" value=<%=@start_date%> class="input-medium" placeholder="from" style="width:90px;" value="<%= @start_date.to_s %>" />
<span class="add-on">-</span>
<input type="text" name="end_date" id="end_date" value=<%=@end_date%> class="input-medium" placeholder="to" style="width:90px;" value="<%= @end_date.to_s %>" />
</div>


<button type="submit" class="btn">Search</button>


</form>
<table class="table table-bordered">
<thead>
<tr>
	<th width="10%">日期</th>
	<th width="5%">标准时</th>
	<th width="5%">金币</th>
	<th colspan="3"  style="text-align:center;">机器</th>
	<th colspan="3"  style="text-align:center;">账号</th>
	<th colspan="3" style="text-align:center;">角色</th>
</tr>
<tr>
<td></td>
<td></td>
<td></td>
<!-- computer -->
<td>Start(次)</td>
<td>No Account</td>
<td>时间(小时)</td>
<!-- account -->
<td>Start(次)</td>
<td>成功(次)</td>
<td>#</td>
<!-- role -->
<td>Pull(次)</td>
<td>Start</td>
<td>成功</td>
</tr>
</thead>
<tbody>
<% @tmp_notes.each do |n|%>
<tr>
<td><%= n.date %></td>
<td></td>
<td></td>
<!-- computer -->
<td><%= n.computer_start %></td>
<td><%= n.computer_no_acount %></td>
<td>#</td>
<!-- account -->
<td><%= n.account_start %></td>
<td><%= n.account_success %></td>
<td>#</td>
<td><%= n.role_dispatch %></td>
<td><%= n.role_start%></td>
<td><%= n.role_success%></td>
</td>
</tr>
<%end%>
</tbody>
</table>

<script type="text/javascript">
$(function(){
	$("#start_date").datepicker({dateFormat:"yy-mm-dd",maxDate:new Date()});
	$("#end_date").datepicker({dateFormat:"yy-mm-dd",maxDate:new Date()});
});
</script>

