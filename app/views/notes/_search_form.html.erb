<form class="form-search" action="<%= search_notes_path %>">
时间:<input type="text" name="min_time" id="start_time" class="input-medium search-query" placeholder="from" value="<%= params[:min_time]%>" />-
<input type="text" name="max_time" id="end_time" class="input-medium search-query" placeholder="to" value="<%= params[:max_time]%>" />
事件:
<input type="text" name="event" class="input-small" placeholder="event" value="<%= params[:event]%>" />
<input type="text" name="code" class="input-small" placeholder="Code" value="<%= params[:code]%>" />
<input type="text" name="role_id" class="input-small" placeholder="角色ID" value="<%= params[:role_id]%>" />
<button type="submit" class="btn">Search</button>
</form>



<script type="text/javascript">
$(function(){
	$("#start_time").datepicker({dateFormat:"yy-mm-dd",maxDate:new Date()});
	$("#end_time").datepicker({dateFormat:"yy-mm-dd",maxDate:new Date()});
});
</script>
