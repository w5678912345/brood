<div class="alert">
<strong>提示信息:</strong>
<button type="button" class="close" data-dismiss="alert">&times;</button>
<span><%= @users.total_entries%>个用户</span> 
</div>
<table class="table table-bordered table-striped">
<thead>
<tr>
<th>ID</th>
<th>Email</th>
<th>姓名</th>
<th>登录次数</th>
<th>超级管理员</th>
<th>创建时间</th>
</tr>
<tbody>
<% @users.each do |user|%>
<tr>
<td><%= user.id %></td>
<td><%= user.email %></td>
<td><%= user.name %></td>
<td><%= user.sign_in_count%></td>
<td><%= user.is_admin ? "Yes" : "No" %></td>
<td><%= time_str user.created_at %></td>
</tr>
<%end%>
</tbody>
</table>
<%= will_paginate @users %> 
