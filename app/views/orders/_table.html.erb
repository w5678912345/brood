<table class="table">
<thead>
<tr>
	<th>ID</th>
	<th>账号</th>
	<th>手机</th>
	<th>触发事件</th>
	<th>结束</th>
	<th>结果</th>
	<th>Msg</th>
	<th>创建时间</th>
</tr>		
</thead>
<tbody>
<% orders.each do |order|%>
<tr>
	<td><%= order.id %></td>
	<td><%= order.account_no %></td>
	<td><%= order.phone_no %></td>
	<td><%= order.trigger_event %></td>
	<td><%= order.finished ? time_str(order.finished_at) : "No" %></td>
	<td><%= order.result %></td>
	<td><%= order.msg %></td>
	<td><%= time_str order.created_at %></td>
</tr>
<%end%>
</tbody>
</table>