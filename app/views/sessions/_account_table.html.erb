<table class="table">
<thead>
<tr>
 <th>ID</th>
 <th>账号</th>
 <th>机器</th>
 <th>IP</th>
 <th>时间</th>
 <th>区</th>
 <th>版本</th>
 <th>用时</th>
 <th>状态</th>
 <th>操作</th>
</tr>
</thead>
<tbody>
<% @sessions.each do |session|%>
<tr>
<td><%= session.id %><%= notes_link_tag(:session_id => session.id )%></td>
<td><%= account_tag session.account %></td>
<td><%= computer_tag session.computer %> </td>
<td><%= session.ip %></td>
<td><%= time_str session.created_at %></td>
<td><%= session.server %></td>
<td><%= session.version %></td>
<td><%= session.ending ?  session.hours : "未结束" %></td>
<td><%= session.status %></td>
<td>#</td>
</tr>
<%end%>
</tbody>
</table>