<%= render "navbar" %>
<fieldset>
<legend>S3 <%= params[:prefix]%></legend>

<ul class="breadcrumb">
	<li><%= link_to "ccnt",s3_versions_path() %> <span class="divider">/</span></li>
	<% if params[:prefix]%>
	<% prefix = params[:prefix]%>
	<% params[:prefix].split("/").each do |p|%>
		<% px = prefix[0...prefix.index(p)+p.length] %>
		<li title="<%= px %>"><%= link_to p,s3_versions_path(:prefix=>px) %> <span class="divider">/</span></li>
	<%end%>
	<%end%>
</ul>

<ul>
<% @objects.as_tree.children.each do |t| %>
<li>
	<%if t.branch? %>
		<i class="icon-folder-close"></i><%= link_to t.prefix,s3_versions_path(:prefix=>t.prefix)%>
	<%else%>
		<i class="icon-file"></i><%= t.key %>
	<%end%>
</li>
<%end%>
</ul>
</fieldset>
