
<table class="table table-bordered table-condensed">
<thead>
<tr>
<th>帐号</th>
<th>服务器</th>
<th>角色数量</th>
<th>绑定机器</th>
<th>在线</th>
<th>创建时间</th>
<th>状态</th>
</tr>
</thead>
<tbody>
<% accounts.each do |account|%>
<tr>
<td><%= account.no %></td>
<td><%= account.server %></td>
<td><%= account.roles_count %></td>
<td><%= account.computers_count %></td>
<td><%= account.online_role_id %></td>
<td><%= time_str account.created_at %></td>
<td><%= account.status %></td>
</tr>
<%end%>
</tbody>
</table>
<%= will_paginate accounts %>