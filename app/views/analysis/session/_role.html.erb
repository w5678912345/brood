<table class="table">
	<thead>
	<tr>
	<th colspan="2">
	</th>
	</tr>
	<tr>
		<th>是否成功</th>
		<th>调度次数</th>
	</tr>
	</thead>
	<tbody>

	<% @role_sessions.each do |rs|%>
	<% success = rs.success ? 1 : 0 %>
	<tr>
		<td><%= rs.success ? "成功" : "失败" %></td>
		<td><%= link_to rs.rscount,role_sessions_path(:success=>success,:date=>@date) %></td>
	</tr>
	<%end%>
	</tbody>
	</table>


	<div class="alert">
<strong>提示信息:</strong>
共<%= @accounts_count %>个账号，调度了<%= @account_sessions_count %>次。
<%= @roles_count%>个角色，调度了<%= @role_sessions_count %>次
</div>

<ul id="myTab" class="nav nav-tabs">
  <li class="active"><a href="#account" data-toggle="tab">账号状态情况</a></li>
  <li><a href="#role" data-toggle="tab">角色成功情况</a></li>
</ul>

<div id="myTabContent" class="tab-content">
<div class="tab-pane fade in active" id="account">
	<%= render "account"%>
</div>
<div class="tab-pane fade" id="role">
	<%= render "role"%>
</div>
</div>
